{{ form_start(form) }}
<div class="row">
    <div class="col-6">
        {{ form_row(form.nom) }}
    </div>
    <div class="col-6">
        {{ form_row(form.prenom) }}
    </div>
</div>
<div class="row">
    <div class="col-6">
        {{ form_row(form.email) }}
    </div>
    <div class="col-6">
        {{ form_row(form.tel) }}
    </div>
</div>
<p class="form-objet">
    Objet de la demande :
<div class="radio-buttons">
    {{ form_row(form.objet) }}
</div>
</p>
<p>
    {{ form_row(form.message) }}
</p>
<p>
    {{ form_row(form.envoyer) }}
</p>

{{ form_end(form) }}